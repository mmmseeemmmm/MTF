<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:System="clr-namespace:System;assembly=mscorlib">
    
    <!-- validation error template -->
    <ControlTemplate x:Key="validationErrorTemplate">
        <DockPanel>
            <TextBlock Foreground="Red" FontWeight="Bold" Text=" !" DockPanel.Dock="Right" />
            <AdornedElementPlaceholder x:Name="ErrorAdorner"></AdornedElementPlaceholder>
        </DockPanel>
    </ControlTemplate>

    <!-- Bse red color -->
    <Color x:Key="BaseRed">Red</Color>
    <SolidColorBrush x:Key="BaseRedBrush" Color="{StaticResource ResourceKey=BaseRed}" />

    <!-- Default yellow color -->
    <Color x:Key="ALYellow">#FEC100</Color>
    <SolidColorBrush x:Key="ALYellowBrush" Color="{StaticResource ResourceKey=ALYellow}" />

    <!-- Default dark yellow color -->
    <Color x:Key="ALDarkYellow">#E3AE00</Color>
    <SolidColorBrush x:Key="ALDarkYellowBrush" Color="{StaticResource ResourceKey=ALDarkYellow}" />

    <!-- Default darker yellow color -->
    <Color x:Key="ALDarkerYellow">#b58b00</Color>
    <SolidColorBrush x:Key="ALDarkerYellowBrush" Color="{StaticResource ResourceKey=ALDarkerYellow}" />

    <!-- Default yellow color -->
    <Color x:Key="ALLightYellow">#FFF9E6</Color>
    <SolidColorBrush x:Key="ALLightYellowBrush" Color="{StaticResource ResourceKey=ALLightYellow}" />
    
    <!-- Default dark yellow color for border -->
    <Color x:Key="ALBorderDarkYellow">#c99a00</Color>
    <SolidColorBrush x:Key="ALBorderDarkYellowBrush" Color="{StaticResource ResourceKey=ALBorderDarkYellow}" />

    <!-- Default black color -->
    <Color x:Key="ALBlack">#081219</Color>
    <SolidColorBrush x:Key="ALBlackBrush" Color="{StaticResource ResourceKey=ALBlack}" />

    <!-- Default  no-transparent black color -->
    <Color x:Key="ALBlackNoTransparent">#262f35</Color>
    <SolidColorBrush x:Key="ALBlackNoTransparentBrush" Color="{StaticResource ResourceKey=ALBlackNoTransparent}" />

    <!-- Default transparent black color -->
    <Color x:Key="ALBlackTransparent">#E0081219</Color>
    <SolidColorBrush x:Key="ALBlackTransparentBrush" Color="{StaticResource ResourceKey=ALBlackTransparent}" />

    <!-- Default green color -->
    <Color x:Key="ALGreen">#86DE75</Color>
    <SolidColorBrush x:Key="ALGreenBrush" Color="{StaticResource ResourceKey=ALGreen}" />

    <!-- Default green color -->
    <Color x:Key="ALLightGreen">#B7E289</Color>
    <SolidColorBrush x:Key="ALLightGreenBrush" Color="{StaticResource ResourceKey=ALLightGreen}" />

    <!-- Default red color -->
    <Color x:Key="ALRed">#FF0000</Color>
    <SolidColorBrush x:Key="ALRedBrush" Color="{StaticResource ResourceKey=ALRed}" />
    
    <!-- Default light red color -->
    <Color x:Key="ALLightRed">#FF5E5E</Color>
    <SolidColorBrush x:Key="ALLightRedBrush" Color="{StaticResource ResourceKey=ALLightRed}" />
    
    <!-- Default silver color -->
    <Color x:Key="ALSilver">#C9C9C9</Color>
    <SolidColorBrush x:Key="ALSilverBrush" Color="{StaticResource ResourceKey=ALSilver}" />
    
    <!-- Default light silver color -->
    <Color x:Key="ALLightSilver">#DDDDDD</Color>
    <SolidColorBrush x:Key="ALLightSilverBrush" Color="{StaticResource ResourceKey=ALLightSilver}" />

    <!-- Default gray color -->
    <Color x:Key="ALGray">#808080</Color>
    <SolidColorBrush x:Key="ALGrayBrush" Color="{StaticResource ResourceKey=ALGray}" />

    <!-- Default white color -->
    <Color x:Key="ALWhite">#FFFFFF</Color>
    <SolidColorBrush x:Key="ALWhiteBrush" Color="{StaticResource ResourceKey=ALWhite}" />
    
    <!-- Sequence colors -->
    <!-- color1 -->
    <Color x:Key="ALColor1">#8DBBD3</Color>
    <SolidColorBrush x:Key="ALColor1Brush" Color="{StaticResource ResourceKey=ALColor1}" />

    <!-- color2 -->
    <Color x:Key="ALColor2">#F18E4A</Color>
