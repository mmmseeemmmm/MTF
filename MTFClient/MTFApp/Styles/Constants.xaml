    <!-- color16 -->
    <Color x:Key="Color16">#C8D337</Color>
    <SolidColorBrush x:Key="ALColor16Brush" Color="{StaticResource ResourceKey=Color16}" />

    <!-- Element border constants -->
    <SolidColorBrush x:Key="ElementBorderBrush" Color="{StaticResource ResourceKey=ALSilver}" />
    <Thickness x:Key="BorderThickness">1</Thickness>

    <!-- Element background constants -->
    <SolidColorBrush x:Key="ElementBackgroundBrush" Color="{StaticResource ResourceKey=ALWhite}" />


    <!-- reportColors  -->
    <Color x:Key="ReportSilverOdd">#fbfbfb</Color>
    <SolidColorBrush x:Key="ReportSilverOddBrush" Color="{StaticResource ResourceKey=ReportSilverOdd}" />

    <Color x:Key="ReportSilverEven">#f2f2f2</Color>
    <SolidColorBrush x:Key="ReportSilverEvenBrush" Color="{StaticResource ResourceKey=ReportSilverEven}" />


</ResourceDictionary>