    <SolidColorBrush x:Key="ALColor4Brush" Color="{StaticResource ResourceKey=ALColor4}" />

    <!-- color5 -->
    <Color x:Key="ALColor5">#E64920</Color>
    <SolidColorBrush x:Key="ALColor5Brush" Color="{StaticResource ResourceKey=ALColor5}" />

    <!-- color6 -->
    <Color x:Key="ALColor6">#108C6A</Color>
    <SolidColorBrush x:Key="ALColor6Brush" Color="{StaticResource ResourceKey=ALColor6}" />

    <!-- color7 -->
    <Color x:Key="ALColor7">#C19DC5</Color>
